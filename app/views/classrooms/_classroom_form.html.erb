<div class="chatroom-form">
  <%= form_for classroom, remote: true do |f| %>
    <div class="input-group">
      <span class="input-group-addon">class_name:</span>
      <%= f.text_field :class_name, class: "form-control"%>
    </div>
    <%= f.submit class: "btn btn-primary", id: "create-classroom" %>
  <% end %>
</div>